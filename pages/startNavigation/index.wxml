<!--pages/startNavigation/index.wxml-->
<view>
  <map
    id="navigationMap"
    style="width: 100%; height: 100vh;"
    latitude="{{latitude}}"
    longitude="{{longitude}}"
    markers="{{markers}}"
    show-location
  >
    <!-- 添加起点和终点的标记 -->
    <marker id="start" iconPath="/images/start.png" longitude="116.397474" latitude="39.908692"></marker>
    <marker id="end" iconPath="/images/end.png" longitude="116.403857" latitude="39.914285"></marker>
  </map>
</view>
